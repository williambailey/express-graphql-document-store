    "@typescript-eslint/parser": "^1.9.0",
    "ava": "^1.4.1",
    "eslint": "^5.16.0",
    "express": "^4.17.0",
    "express-graphql": "^0.8.0",
    "graphql": "^14.2.0",
    "supertest": "^4.0.2",
